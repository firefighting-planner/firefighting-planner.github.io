{
  "name": "firefighting-planner",
  "description": "Collaborative planning tool for fire fighting against wildfire",
  "version": "0.1.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/watilde/firefighting-planner"
  },
  "homepage": "https://firefighting-planner.dev",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint '{src,test,scripts,e2e}/**/*.js' --max-warnings=0",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "postinstall": "commitizen init cz-conventional-changelog"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@nasaworldwind/worldwind": "^0.9.0",
    "lodash.merge": "^4.6.2",
    "material-table": "^1.53.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "prettier": "^1.18.2"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint '{src,test,scripts,e2e}/**/*.js' --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-push": "npm run lint && npm run test:coverage"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
